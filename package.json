{
  "name": "ipyaggrid",
  "version": "0.2.1",
  "description": "A jupyter widget embedding the ag-grid library",
  "author": "Louis Raison, oscar6echo",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/DGothrek/ipyaggrid"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets"
  ],
  "files": [
    "src/**/*.js",
    "dist/*.js",
    "src/styles/**/*.css",
    "src/styles/icons/*.svg",
    "style/index.js"
  ],
  "jupyterlab": {
    "extension": "src/labplugin",
    "outputDir": "ipyaggrid/labextension"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:prod": "jlpm run clean && jlpm run build:lib && jlpm run build:labextension",
    "clean": "jlpm run clean:lib",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "clean:labextension": "rimraf ipyaggrid/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "cleanall": "npm run clean && rimraf node_modules/",
    "dev": "webpack --mode development --watch",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "format": "prettier --write '**/*.{js,jsx}'",
    "install:extension": "jlpm run build",
    "lint": "eslint '**/*.{js,jsx}' --quiet",
    "prepare": "webpack --mode production",
    "test": "echo \"Error: no test specified toto\"",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^3.0.0",
    "@jupyter-widgets/controls": "^2.0.0",
    "ag-grid-community": "21.1.0",
    "ag-grid-enterprise": "21.1.0",
    "d3": "^5.9.7",
    "lodash": "^4.17.15",
    "pako": "^1.0.10"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^3.1.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mkdirp": "^1.0.3",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "sass": "^1.22.7",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-colorize-loader": "^0.1.2",
    "typescript": "~4.1.3",
    "url-loader": "^2.1.0",
    "watch": "^1.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.6"
  },
  "styleModule": "style/index.js"
}